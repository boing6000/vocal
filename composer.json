{
    "name": "sjdaws/vocal",
    "description": "Recursive functionality for Eloquent in Laravel 4 and 5",
    "version": "2.0.0-dev",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "laravel4", "laravel5", "eloquent", "recursive", "hydration", "validation"],
    "authors": [
        {
            "name": "Scott Dawson",
            "email": "scott@josc.com.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": ">=4.0",
        "illuminate/hashing": ">=4.0",
        "illuminate/support": ">=4.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ],
    "support": {
        "issues": "https://github.com/sjdaws/vocal/issues",
        "source": "https://github.com/sjdaws/vocal"
    },
    "autoload": {
        "psr-0": {
            "Sjdaws\\Vocal": "src/"
        },
        "psr-4": {
            "Sjdaws\\Vocal\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
